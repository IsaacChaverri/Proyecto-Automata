if(not initialized_dependent_variables){
    initialized_dependent_variables = true;
    //Create Page Name Box
    var page_name_box_x = x;
    var page_name_box_height = sprite_get_height(spr_page_name_box);
    var page_name_box_y = y+(page_name_box_height/2);
    var page_name_box_depth = depth-10;
    page_name_box = scr_instance_create_depth(page_name_box_x,page_name_box_y,page_name_box_depth,obj_page_name_box);
    page_name = page_data.creator_user_data.name;
    with(page_name_box){
        page_name = other.page_name;
    }
    //Create Levels
    var levels_data_list = page_data.levels_data_list;
    var levels_data_list_size = ds_list_size(levels_data_list);
    for(var i=0;i&lt;levels_data_list_size;i++){
        var temp_level_data = ds_list_find_value(levels_data_list,i);
        var new_level_button_x = x;
        var page_name_box_height = sprite_get_height(spr_page_name_box);
        var firts_level_button_y_offset = page_name_box_height*1.25;
        var level_button_height = sprite_get_height(spr_level_button);
        levels_y_difference = level_button_height*1.5;
        var new_level_button_y = page_name_box.y+(page_name_box_height/2)+firts_level_button_y_offset+(i*levels_y_difference);
        var new_level_button_depth = depth-1;
        var new_level_button_object_index = obj_level_button;
        if(page_data.creator_user_data.email == CURRENT_USER_DATA.email){
            new_level_button_object_index = obj_administrator_level_button;
        }
        var new_level_button = scr_instance_create_depth(new_level_button_x,new_level_button_y,new_level_button_depth,new_level_button_object_index);
        var first_unused_letter_index = 0;
        var new_level_button_letter = ds_list_find_value(unused_letters_list,first_unused_letter_index);
        ds_list_delete(unused_letters_list,first_unused_letter_index);
        var new_level_button_previous_level_button = noone;
        var previous_level_button = ds_list_find_value(level_buttons_list,i-1);
        if(previous_level_button != undefined and previous_level_button != noone){
            new_level_button_previous_level_button = previous_level_button;
            new_level_button_previous_level_button.next_level_button = new_level_button;
        }
        with(new_level_button){
            letter = new_level_button_letter;
            level_data = temp_level_data;
            id.previous_level_button = new_level_button_previous_level_button;
            page = other.id;
            script = scr_level_button;
        }
        ds_list_add(level_buttons_list,new_level_button);
    }
    //Create Administrator Add Button
    if(page_data.creator_user_data.email == CURRENT_USER_DATA.email){
        var level_buttons_list_size = ds_list_size(level_buttons_list);
        var last_level_button = ds_list_find_value(level_buttons_list,level_buttons_list_size-1);
        var administrator_add_button_x = last_level_button.x;
        var administrator_add_button_y = last_level_button.y+levels_y_difference;
        var administrator_add_button_depth = last_level_button.depth;
        administrator_add_button = scr_instance_create_depth(administrator_add_button_x,administrator_add_button_y,administrator_add_button_depth,obj_administrator_add_button);
        with(administrator_add_button){
            page = other.id;
            id.last_level_button = last_level_button;
        }
    }
}
