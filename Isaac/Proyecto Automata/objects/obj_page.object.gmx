            <string>///Update Positions
//Page Name Box
page_name_box.x = x;
//Levels
var level_buttons_max_y = 0;
var level_buttons_min_y = 0;
var level_buttons_list_size = ds_list_size(level_buttons_list);
var objects_list_size = level_buttons_list_size;
if(administrator_add_button != noone){
    objects_list_size += 1;
}
if(objects_list_size &gt; max_level_buttons_on_screen){
    var border_level_button = ds_list_find_value(level_buttons_list,max_level_buttons_on_screen-1);
    var last_level_button = ds_list_find_value(level_buttons_list,level_buttons_list_size-1);
    var last_object = last_level_button;
    if(administrator_add_button != noone){
        last_object = administrator_add_button;
    }
    level_buttons_min_y = -abs(last_object.y-border_level_button.y);
    
}
level_buttons_y = clamp(level_buttons_y,level_buttons_min_y,level_buttons_max_y);

for(var i=0;i&lt;level_buttons_list_size;i++){
    var temp_level_button = ds_list_find_value(level_buttons_list,i);
    temp_level_button.x = x;
    temp_level_button.y = temp_level_button.ystart+level_buttons_y;
}
