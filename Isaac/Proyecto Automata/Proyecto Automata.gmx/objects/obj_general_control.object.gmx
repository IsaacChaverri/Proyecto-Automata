        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Update Current Gesture
switch(CURRENT_GESTURE){
    case "OFF":
        GESTURE_IS_DOUBLE = false;
        if(expecting_second_touch){
            if(mouse_check_button(mb_left)){
                CURRENT_GESTURE = "TOUCH";
                if(point_distance(mouse_x,mouse_y,start_touch_x,start_touch_y) &lt; double_tap_distance_threshold){
                    GESTURE_IS_DOUBLE = true;
                }else{
                    start_touch_time = current_time;
                }
                start_touch_x = mouse_x;
                start_touch_y = mouse_y;
                expecting_second_touch = false;
            }
            if(current_time-start_touch_time &gt; double_tap_time_threshold){
                expecting_second_touch = false;
            }
        }else{
            if(mouse_check_button(mb_left)){
                CURRENT_GESTURE = "TOUCH";
                start_touch_time = current_time;
                start_touch_x = mouse_x;
                start_touch_y = mouse_y;
            }
        }
        break;
    case "TOUCH":
        if(mouse_check_button_released(mb_left)){
            CURRENT_GESTURE = "TAP";
        }
        if(point_distance(mouse_x,mouse_y,start_touch_x,start_touch_y) &gt; drag_distance_threshold){
            CURRENT_GESTURE = "DRAG";
            mouse_previous_x = mouse_x;
            mouse_previous_y = mouse_y;
        }
        break;
    case "TAP":
        CURRENT_GESTURE = "OFF";
        if(not GESTURE_IS_DOUBLE){
            expecting_second_touch = true;
        }
        break;
    case "DRAG":
        if(mouse_check_button_released(mb_left)){
            CURRENT_GESTURE = "OFF";
            var drag_speed = point_distance(mouse_x,mouse_y,mouse_previous_x,mouse_previous_y);
            if(drag_speed &gt; swipe_speed_threshold){
                CURRENT_GESTURE = "SWIPE";
                SWIPE_HORIZONTAL_SPEED = mouse_x-mouse_previous_x;
                SWIPE_VERTICAL_SPEED = mouse_y-mouse_previous_y;
                SWIPE_SPEED = drag_speed;
            }
        }
        mouse_previous_x = mouse_x;
        mouse_previous_y = mouse_y;
        break;
    case "SWIPE":
        CURRENT_GESTURE = "OFF";
        break;
}
</string>
          </argument>
        </arguments>
      </action>
