    //Create Number Buttons
    var number_buttons_amount = 12;
    var max_horizontal_number_buttons = 3;
    var number_buttons_current_row = 0;
    var number_buttons_current_column = 0;
    var number_button_width = sprite_get_width(spr_number_dial_window_number_button);
    var number_buttons_x_distance = number_button_width*1.25;
    var left_number_buttons_x = x-number_buttons_x_distance;
    var number_button_height = sprite_get_height(spr_number_dial_window_number_button);
    var number_buttons_y_distance = number_button_height*1.25;
    var top_number_buttons_y_offset = values_y_offset*2;
    var top_number_buttons_y = y-(sprite_height/2)+values_y_offset+top_number_buttons_y_offset;
    number_buttons_list = ds_list_create();
    for(var number_buttons_counter=0;number_buttons_counter&lt;number_buttons_amount;number_buttons_counter++){
        if(number_buttons_current_column &gt;= max_horizontal_number_buttons){
            number_buttons_current_column = 0;
            number_buttons_current_row++;
        }
        var new_number_button_x = left_number_buttons_x+(number_buttons_x_distance*number_buttons_current_column);
        var new_number_button_y = top_number_buttons_y+(number_buttons_y_distance*number_buttons_current_row);
        var new_number_button_depth = depth-1;
        var new_number_value = string(number_buttons_counter+1);
        var new_number_script = scr_number_dial_window_number_button;
        if(number_buttons_counter &gt; 8){
            switch(number_buttons_counter){
                case 9:
                    new_number_value = "e";
                    new_number_script = scr_number_dial_window_special_number_button;
                    break;
                case 10:
                    new_number_value = "0";
                    break;
                case 11:
                    new_number_value = "E";
                    new_number_script = scr_number_dial_window_special_number_button;
                    break;
            }
        }
        var new_number_button = scr_instance_create_depth(new_number_button_x,new_number_button_y,new_number_button_depth,obj_number_dial_button);
        with(new_number_button){
            number_dial_window = other.id;
            value = new_number_value;
            script = new_number_script;
        }
        ds_list_add(number_buttons_list,new_number_button);
        number_buttons_current_column++;
    }
    //Create Cancel Button
    var cancel_button_width = sprite_get_width(spr_number_dial_window_cancel_button);
    var cancel_button_x = left_number_buttons_x-(number_button_width/2)+(cancel_button_width/2);
    var last_number_button = ds_list_find_value(number_buttons_list,number_buttons_amount-1);
    var cancel_button_y_offset = number_buttons_y_distance+(number_buttons_y_distance/4);
    var cancel_button_y = last_number_button.y+cancel_button_y_offset;
    var cancel_button_depth = depth-1;
    cancel_button = scr_instance_create_depth(cancel_button_x,cancel_button_y,cancel_button_depth,obj_text_button);
    with(cancel_button){
        number_dial_window = other.id;
        sprite_index = spr_number_dial_window_cancel_button;
        image_speed = 0;
        text = "Cancelar";
        text_font = fnt_arial_18;
        script = scr_number_dial_window_cancel_button;
    }
    //Create Accept Button
    var accept_button_width = sprite_get_width(spr_number_dial_window_accept_button);
    var right_number_buttons_x = last_number_button.x;
    var accept_button_x = right_number_buttons_x+(number_button_width/2)-(accept_button_width/2);
    var accept_button_y = cancel_button_y;
    var accept_button_depth = depth-1;
    accept_button = scr_instance_create_depth(accept_button_x,accept_button_y,accept_button_depth,obj_number_dial_window_accept_button);
    with(accept_button){
        number_dial_window = other.id;
